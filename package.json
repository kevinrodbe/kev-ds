{
  "name": "kevinrodbe",
  "private": true,
  "version": "0.0.0",
  "description": "deploy",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "keywords": [
    "frontend"
  ],
  "author": "Team",
  "license": "ALR",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "pre-commit": "concurrently -n lint-code,test-code -c \"auto\" \"pnpm run lint:all\" \"pnpm run test:all\"",
	"commit": "pnpm pre-commit && cz",
    "lint:all": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint --fix",
    "test:all": "nx run-many --target=test",
    "build:all": "nx run-many --target=build",
    "add:node": "plop --plopfile plopfile.mjs && pnpm i",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@commitlint/format": "^17.4.4",
    "@commitlint/types": "^17.4.4",
    "@jscutlery/semver": "^3.0.0",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "nx": "16.3.2",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "volta": {
    "node": "18.16.0"
  }
}
